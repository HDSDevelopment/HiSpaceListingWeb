<body>
	<!--=================================
	header -->
	<header class="header">
		<div class="topbar">
			<div class="container-fluid">
				<div class="row">
					<div class="col-12">
						<div class="d-block d-md-flex align-items-center text-center">
							<div class="mr-3 d-inline-block">
								<a href="tel:1-800-555-1234"><i class="fa fa-phone mr-2 fa fa-flip-horizontal"></i>+91 98403 52595 </a>
							</div>
							<div class="mr-auto d-inline-block">
							</div>
							<div class="dropdown d-inline-block pl-2 pl-md-0">
								<a class="dropdown-toggle" href="#" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></a>
							</div>
							<div class="social d-inline-block">
								<ul class="list-unstyled">
									<li><a href="#"> <i class="fab fa-facebook-f"></i> </a></li>
									<li><a href="#"> <i class="fab fa-twitter"></i> </a></li>
									<li><a href="#"> <i class="fab fa-linkedin"></i> </a></li>
									<li><a href="#"> <i class="fab fa-pinterest"></i> </a></li>
									<li><a href="#"> <i class="fab fa-instagram"></i> </a></li>
								</ul>
							</div>
							<div class="login d-inline-block">
								@if (ViewBag.UserId != null && ViewBag.UserId >= 0)
								{
									<div class="user-alert bg-sec"></div>
									<ul class="nav navbar-nav">
										<li class="nav-item dropdown">
											<a class="nav-link dropdown-toggle" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
												@ViewBag.UserCompanyName <i class="fa fa-user pl-2"></i>
											</a>
											<ul class="dropdown-menu">
												<li><a class="dropdown-item" href="@Url.Action( "Logout","User")">Sign out</a></li>
											</ul>
										</li>
									</ul>
								}
								else
								{
									<div class="user-alert bg-pry"></div>
									<a class="modal-link__login" data-targeturl="@Url.Action("LoginPartialView","User")" href="javascript:function() { return false; }"> sign in<i class="fa fa-user pl-2"></i></a>
								}
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<nav class="navbar navbar-light bg-white navbar-static-top navbar-expand-lg header-sticky">
			<div class="container-fluid">
				<button type="button" class="navbar-toggler" data-toggle="collapse" data-target=".navbar-collapse"><i class="fas fa-align-left"></i></button>
				<a class="navbar-brand" href="@Url.Action( "Index","Website")">
					<img class="img-fluid" src="~/images/logo.png" alt="logo">
				</a>
				<div class="navbar-collapse collapse justify-content-center">
					<ul class="nav navbar-nav">
						<li class="nav-item  active">
							<a class="nav-link" href="@Url.Action( "Index","Website")" id="navbarDropdown">Home</a>
						</li>

						<li class="nav-item dropdown">
							<a class="nav-link dropdown-toggle" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
								Listing <i class="fas fa-chevron-down fa-xs"></i>
							</a>
							<ul class="dropdown-menu">
								<li><a class="dropdown-item" href="@Url.Action( "PropertyList","Website")">Commercial</a></li>
								<li><a class="dropdown-item" href="@Url.Action( "PropertyList","Website")">Coworking</a></li>
								<li><a class="dropdown-item" href="@Url.Action( "PropertyList","Website")">RE-Professionals</a></li>
							</ul>
						</li>
						<li class="nav-item dropdown ">
							<a class="nav-link" href="@Url.Action( "About","Website")">About</a>
						</li>
						<li class="nav-item  ">
							<a class="nav-link" href="@Url.Action( "Contact","Website")" id="navbarDropdown">Contact</a>
						</li>
						@if (ViewBag.UserId != null && ViewBag.UserId == 0)
						{
							<li class="nav-item dropdown">
								<a class="nav-link dropdown-toggle" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
									Dashboard <i class="fas fa-chevron-down fa-xs"></i>
								</a>
								<ul class="dropdown-menu">
									<li><a class="dropdown-item" href="@Url.Action( "AdminLister","Admin")">Operators</a></li>
								</ul>
							</li>
						}
						else if (ViewBag.UserId != null && ViewBag.UserId > 0)
						{
							<li class="nav-item dropdown">
								<a class="nav-link dropdown-toggle" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
									Dashboard <i class="fas fa-chevron-down fa-xs"></i>
								</a>
								<ul class="dropdown-menu">
									<li><a class="dropdown-item" href="@Url.Action( "ListingTable","Listing",new { UserID = ViewBag.UserId, UserType = ViewBag.UserType})">My Listing</a></li>
								</ul>
							</li>
						}
					</ul>
				</div>
				@*<div class="add-listing d-none d-sm-block">
						<a class="btn btn-primary btn-md" href="@Url.Action( "ListingTable","Listing",new { UserID = ViewBag.UserId})"> <i class="fa fa-plus-circle"></i>Add listing </a>
					</div>*@
			</div>
		</nav>
	</header>
	<!--=================================
	 header -->
	<!--=================================
	 Modal login -->
	<!-- The below code is for the modal dialog login section-->
	<div id="modal-container__login" class="modal fade" tabindex="-1" role="dialog">
		<a href="#close" title="Close" class="modal-close-btn">X</a>
		<div class="modal-content">
			<div class="modal-body"></div>
		</div>
	</div>
	<!--=================================
	  Modal login -->
	<!--=================================
	Modal signup -->
	<!-- The below code is for the modal dialog signup section-->
	<div id="modal-container__signup" class="modal fade" tabindex="-1" role="dialog">
		<a href="#close" title="Close" class="modal-close-btn">X</a>
		<div class="modal-content">
			<div class="modal-body"></div>
		</div>
	</div>
	<!--=================================
	  Modal signup -->
