@{
	ViewData["Title"] = "FilterList";
	Layout = "~/Views/Shared/_WebLayout.cshtml";
	@using Microsoft.AspNetCore.Http
	@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
}
@model HiSpaceListingWeb.ViewModel.PropertyOperatorPeopleAndFilterMenuViewModel

<div class="property-list nav-padding container">
	<div class="bg-light">
		<div class="container">
			<div class="row">
				<div class="col-12">
					<ol class="breadcrumb mb-0">
						<li class="breadcrumb-item"><a href="@Url.Action("Index", "Website")"> <i class="fas fa-home"></i> </a></li>
						@*<li class="breadcrumb-item"><i class="fas fa-chevron-right"></i><a href="@Url.Action( "PropertyList","Website")"> All Property List </a></li>*@
						<li class="breadcrumb-item active"> <i class="fas fa-chevron-right"></i> <span> Property </span></li>
					</ol>
				</div>
			</div>
		</div>
	</div>
	@*Nav bar section*@
	<ul class="nav nav-tabs nav-tabs-02" id="myTab" role="tablist">
		<li class="nav-item">
			<a class="nav-link active" id="tab-01-tab" data-toggle="tab" href="#tab-01" role="tab" aria-controls="tab-01" aria-selected="true"><span class="icon fab flaticon-building"></span> Property</a>
		</li>
		<li class="nav-item">
			<a class="nav-link" id="tab-02-tab" data-toggle="tab" href="#tab-02" role="tab" aria-controls="tab-02" aria-selected="false"><span class="icon fab flaticon-building-3"></span> Operator</a>
		</li>
		<li class="nav-item">
			<a class="nav-link" id="tab-03-tab" data-toggle="tab" href="#tab-03" role="tab" aria-controls="tab-03" aria-selected="false"><span class="icon fab flaticon-agent"></span> RE Professionals</a>
		</li>
	</ul>
	<div class="tab-content" id="myTabContent">
		@*property list*@
		<div class="tab-pane fade show active" id="tab-01" role="tabpanel" aria-labelledby="tab-01-tab">
			@Html.Partial("_PropertyMainListPartialView", Model)
		</div>
		@*operator list*@
		<div class="tab-pane fade" id="tab-02" role="tabpanel" aria-labelledby="tab-02-tab">
			@Html.Partial("_OperatorMainListPartialView", Model)
		</div>
		@*people list*@
		<div class="tab-pane fade" id="tab-03" role="tabpanel" aria-labelledby="tab-03-tab">
			@Html.Partial("_ProfessionalMainListPartialView", Model)
		</div>
	</div>
</div>


@section Scripts {
	<script src="~/js/CustomFilters.js"></script>
}


